# Completion model
LPG_AI_GENERATOR_MODEL_NAME=gemma:7B

# Embedding model
LPG_AI_EMBEDDING_MODEL=mxbai-embed-large:latest
LPG_AI_EMBEDDING_DIM=1024

# Ollama config
LPG_AI_OLLAMA_API_URL=http://127.0.0.1:11434

# Vectorstore
LPG_AI_COLLECTION_NAME=documents

# Mode and server settings
LPG_AI_DEV_MODE=false
LPG_AI_API_HOST=0.0.0.0
LPG_AI_API_PORT=8003

# CORS for prod frontend
LPG_AI_CORS_ORIGINS=https://support.gpt.strategies.dev

# Environment label
LPG_AI_ENVIRONMENT=production

# Logging and database
LPG_AI_LOG_LEVEL=INFO
LPG_AI_DATABASE_URL=sqlite:///./lpg_ai.db

# Security
LPG_AI_SECRET_KEY=your-production-secret-key

# Rate limiting
LPG_AI_RATE_LIMIT_PER_MINUTE=60